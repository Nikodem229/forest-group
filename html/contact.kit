<!DOCTYPE html>
<html lang="pl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest Group - Najpięknieszy park w Twojej okolicy</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/63b1d35ea4.js" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="dist/css/style.min.css">
</head>
<body>
    <!-- @include 'nav.kit' -->
    <div class="container">
        <header class="header">
            <div class="hero-img-offers">
                <div class="hero-shadow"></div>
                <div class="hero-text-offers p-normal">
                    <h1 class="h1-offers-page">KONTAKT</h1>
                </div>
            </div>
        </header>

        <section class="contact p-normal pt-5" id="offer">
            <p class="mb-0 mt-5 text-center offer-subtitle">Skontaktuj się z nami!</p>

            <div class="form-box">
                <div class="image-box">
                    <img src="../dist/img/forest-2942477_640.jpg" alt="forest" class="img-fluid w-100">
                </div>
                <form class="contact-form">
                    <input type="mail" class="input-mail" placeholder="Podaj adres e-mail">
                    <textarea class="textarea" placeholder="Wpisz treść wiadomości..."></textarea>
                    <button class="more-offers-btn mt-0 w-50">Wyślij</button>
                </form>
            </div>
            
            
        </section>

        <!-- @include 'footer.kit' -->

    </div>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
    <script src="dist/js/script.min.js"></script>
</body>
</html>